{
  "root": true,
  "parser": "vue-eslint-parser",
  "extends": [
    "plugin:vue/essential",
    "airbnb-base"
  ],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "no-param-reassign": 0,
    "generator-star-spacing": 0,
    "import/extensions": ["error", "always", {
      "js": "never",
      "vue": "never"
    }]
  }
}
